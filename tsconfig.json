{
	"compilerOptions": {
		"strict": true,
		"allowJs": true,
		"target": "ESNext",
		"module": "ESNext",
		"declaration": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"moduleResolution": "Node",
		"emitDeclarationOnly": true,
		"useDefineForClassFields": true,
		"lib": ["ESNext", "DOM", "DOM.Iterable"]
	},
	"include": [
		"lib/TableView.vue",
		"lib/index.js"
	]
}
